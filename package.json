    "@vitejs/plugin-react": "4.4.1",
    "dotenv": "16.5.0",
    "globals": "16.0.0",
    "i18next": "25.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "15.6.1",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "vite-node": "3.1.2",
    "wouter": "3.7.0",
    "zustand": "5.0.3"
